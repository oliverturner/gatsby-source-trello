{
  "name": "@oliverturner/gatsby-source-trello",
  "description": "Gatsby source plugin for Trello",
  "version": "1.0.0",
  "author": "Oliver Turner",
  "license": "ISC",
  "keywords": [
    "gatsby",
    "trello"
  ],
  "main": "gatsby-node.js",
  "repository": "https://github.com/Necmttn/gatsby-source-trello",
  "dependencies": {
    "axios": "0.18.0",
    "cross-env": "^5.1.0",
    "query-string": "6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.46",
    "@babel/core": "7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/runtime": "7.0.0-beta.46",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "dotenv": "5.0.1",
    "eslint": "4.19.1",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.0.5",
    "prettier": "1.12.1"
  },
  "jest": {
    "verbose": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "lint": "eslint --fix ./src",
    "build": "babel src --out-dir . --ignore src/__tests__",
    "test": "jest --watch",
    "watch": "babel -w src --out-dir . --ignore src/__tests__",
    "precommit": "lint-staged",
    "prepublish": "cross-env NODE_ENV=production npm run build"
  }
}
